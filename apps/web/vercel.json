{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://agentos-backend.railway.app/api/:path*" },
    { "source": "/ws/:path*", "destination": "wss://agentos-backend.railway.app/ws/:path*" }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://agentos-backend.railway.app",
    "NEXT_PUBLIC_WS_URL": "wss://agentos-backend.railway.app",
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_anon_key"
  }
}
